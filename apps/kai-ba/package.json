{
  "name": "kai-ba",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest --coverage",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "rebuild": "rimraf ./node_modules && yarn install",
    "build": "yarn clean && tsc  --build --verbose",
    "start": "yarn build && fastify start -l info dist/app.js",
    "watch:ts": "tsc --watch",
    "dev": "yarn build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"yarn watch:ts\" \"yarn dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/auth": "^4.6.1",
    "@fastify/autoload": "^5.0.0",
    "@fastify/compress": "^7.0.3",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/redis": "^6.2.0",
    "@fastify/secure-session": "^7.5.1",
    "@fastify/sensible": "^5.0.0",
    "@fastify/static": "^7.0.4",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "awilix": "^11.0.0",
    "close-with-grace": "^1.3.0",
    "fastify": "^4.26.1",
    "fastify-cli": "^6.2.1",
    "fastify-plugin": "^4.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "c8": "^9.0.0",
    "concurrently": "^8.2.2",
    "fastify-tsconfig": "^2.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.2.2"
  }
}
